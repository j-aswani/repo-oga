{
  "name": "camara-node-sdk",
  "version": "1.0.0",
  "description": "Public SDK used by applications to operate with the OpenGateway platform.",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "npm run prepare",
    "preprepare": "rm -rf dist",
    "prepare": "tsc -p tsconfig-esm.json && tsc -p tsconfig-cjs.json",
    "format": "prettier --write ."
  },
  "dependencies": {
    "axios": "^1.3.5",
    "lru-cache": "^9.1.1",
    "memoizee": "^0.4.15",
    "node-jose": "^2.2.0",
    "uuid": "^9.0.0"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5",
    "printWidth": 120,
    "tabWidth": 2,
    "semi": true
  }
}
